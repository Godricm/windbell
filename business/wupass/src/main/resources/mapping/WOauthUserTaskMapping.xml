<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.afterturn.boot.wupass.im.repository.WOauthUserTaskRepository">

    <resultMap id="BaseResultMap" type="cn.afterturn.boot.wupass.im.model.WOauthUserTaskModel">
        <result column="ID" property="id"/>
        <result column="TENANT_ID" property="tenantId"/>
        <result column="CRT_USER_ID" property="crtUserId"/>
        <result column="CRT_TIME" property="crtTime"/>
        <result column="MDF_USER_ID" property="mdfUserId"/>
        <result column="MDF_TIME" property="mdfTime"/>
        <result column="TASK_ID" property="taskId"/>
        <result column="OAUTH_USER_ID" property="oauthUserId"/>
        <result column="TASK_STATUS" property="taskStatus"/>
        <result column="DESC" property="desc"/>
        <result column="EVALUATE" property="evaluate"/>
    </resultMap>


    <sql id="select_content">
        <if test="ew.entity.id != null and ew.entity.id != '' ">
            AND t.ID = #{ew.entity.id}
        </if>
        <if test="ew.entity.tenantId != null and ew.entity.tenantId != '' ">
            AND t.TENANT_ID = #{ew.entity.tenantId}
        </if>
        <if test="ew.entity.crtUserId != null and ew.entity.crtUserId != '' ">
            AND t.CRT_USER_ID = #{ew.entity.crtUserId}
        </if>
        <if test="ew.entity.crtTime != null and ew.entity.crtTime != '' ">
            AND t.CRT_TIME = #{ew.entity.crtTime}
        </if>
        <if test="ew.entity.mdfUserId != null and ew.entity.mdfUserId != '' ">
            AND t.MDF_USER_ID = #{ew.entity.mdfUserId}
        </if>
        <if test="ew.entity.mdfTime != null and ew.entity.mdfTime != '' ">
            AND t.MDF_TIME = #{ew.entity.mdfTime}
        </if>
        <if test="ew.entity.taskId != null and ew.entity.taskId != '' ">
            AND t.TASK_ID = #{ew.entity.taskId}
        </if>
        <if test="ew.entity.oauthUserId != null and ew.entity.oauthUserId != '' ">
            AND t.OAUTH_USER_ID = #{ew.entity.oauthUserId}
        </if>
        <if test="ew.entity.taskStatus != null and ew.entity.taskStatus != '' ">
            AND t.TASK_STATUS = #{ew.entity.taskStatus}
        </if>
        <if test="ew.entity.desc != null and ew.entity.desc != '' ">
            AND t.DESC = #{ew.entity.desc}
        </if>
        <if test="ew.entity.evaluate != null and ew.entity.evaluate != '' ">
            AND t.EVALUATE = #{ew.entity.evaluate}
        </if>
    </sql>

</mapper>